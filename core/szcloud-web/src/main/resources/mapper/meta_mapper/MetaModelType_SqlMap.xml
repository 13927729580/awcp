<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="org.szcloud.framework.metadesigner.core.domain.MetaModelType">
	<!-- <cache type="org.mybatis.caches.memcached.LoggingMemcachedCache" /> -->
	<resultMap id="rm" type="org.szcloud.framework.metadesigner.core.domain.MetaModelType">
		<id property="id" jdbcType="BIGINT" column="id"/>
		<result property="typeName" jdbcType="VARCHAR" column="typeName"/>
		<result property="typeName" jdbcType="VARCHAR" column="typeName"/>
	</resultMap>
	<sql id="Base_Column_List">
		id,typeName,typeCode
	</sql>
	
	<select id="getAll" resultMap="rm">
		select <include refid="Base_Column_List"/> from fw_mm_metamodeltype
	</select>
	
	<insert id="insert" parameterType="Object">
		<selectKey resultType="java.lang.Long" order="AFTER"
			keyProperty="id">
			SELECT LAST_INSERT_ID()
		</selectKey>
		insert into fw_mm_metamodeltype(typeName,tyepCode)
		value(#{typeName},#{typeCode})
	</insert>
	
	<!-- 根据属性查询 -->
	<select id="get" parameterType="Object" resultMap="rm">
		select <include refid="Base_Column_List"/> from fw_mm_metamodeltype
	</select>
	
	<update id="update" parameterType="org.szcloud.framework.metadesigner.core.domain.MetaModelType">
		UPDATE fw_mm_metamodeltype SET
			typeName=#{tyepeName},
			typeName=#{typeName},
		WHERE 
			id = #{id} 
	</update>
	
</mapper>

